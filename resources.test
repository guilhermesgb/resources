#### TESTES para RECURSOS ####

#criar tipo de recurso
expectError “Nome e descrição inválidos!" criarTipoRecurso nome= descricao=
expectError “Nome inválido!" criarTipoRecurso nome= descricao=wewe
expectError “Descrição inválido!" criarTipoRecurso nome=sdsd descricao=
idTR1=criarTipoRecurso nome=teste1 descricao=teste2

#remover tipo de recurso sem atributos
removerTipoRecurso  idTipoRecurso=${idTR1}
expectError “Tipo de recurso não encontrado!” removerTipoRecurso  idTipoRecurso=${idTR1}
idTR1=criarTipoRecurso  nome=teste1 descricao=teste2

#alterar tipo de recurso

alterarTipoRecurso  idTipoRecurso=${idTR1} nome=teste1 descricao=teste2 atributos=“{{nome=’nome’,tipo:’NUMERO’,naoVazio=’true'}}"

expectError “Já existe um atributo com esse nome!” addAtributo  idTipoRecurso=${idTR1} nome=teste1 descricao=teste2  atributos=“{{nome=’nome’,tipo:’NUMERO’,naoVazio=’true'},{nome=’nome’,tipo:’NUMERO’,naoVazio=’true'}}"

expectError “Atributo ‘nome' não pode ser vazio!” alterarTipoRecurso  idTipoRecurso=${idTR1} nome=teste1 descricao=teste2 atributos=“{{nome=’’,tipo:’NUMERO’,naoVazio=’true'}}”

expectError “Atributo 'tipo' não pode ser vazio!” alterarTipoRecurso  idTipoRecurso=${idTR1} nome=teste1 descricao=teste2 atributos=“{{nome=’sdsds’,tipo:’’,naoVazio=’true'}}”

expectError “Atributo 'naoVazio' não pode ser vazio!” alterarTipoRecurso  idTipoRecurso=${idTR1} nome=teste1 descricao=teste2 atributos=“{{nome=’sdsds’,tipo:’TEXTO’,naoVazio=’'}}"

alterarTipoRecurso  idTipoRecurso=${idTR1} nome=teste1 descricao=teste2 atributos=“{{nome=’nome’,tipo:’NUMERO’,naoVazio=’false'},{nome=’texto’,tipo:’TEXTO’,naoVazio=’true'}}”

alterarTipoRecurso  idTipoRecurso=${idTR1} nome=teste1 descricao=teste2 atributos=“{{nome=’nome’,tipo:’NUMERO’,naoVazio=’true'}}”

alterarTipoRecurso  idTipoRecurso=${idTR1} nome=teste1 descricao=teste2 atributos=“{{nome=’dia’,tipo:’DATA’,naoVazio=’true'}}"

#remover tipo de recurso

removerTipoRecurso  idTipoRecurso={$idTR1}
expectError “Tipo de recurso não encontrado!” removerTipoRecurso  idTipoRecurso=${idTR1}

